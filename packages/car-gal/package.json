{
  "name": "@ev-core/car-gal",
  "description": "A car gallery plugin for the EV Core framework.",
  "version": "0.6.5",
  "types": "dist/esm/index.d.ts",
  "module": "dist/esm/index.js",
  "main": "dist/cjs/index.js",
  "style": "dist/css/index.css",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./dist/css/index.css": "./dist/css/index.css"
  },
  "scripts": {
    "clean": "rimraf dist && mkdir dist",
    "build": "pnpm run build:cjs && pnpm run build:esm && pnpm run build:scss",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:esm:ts2mjs": "tsc -p tsconfig.esm.json && ts2mjs dist/esm",
    "build:scss": "sass --no-source-map styles.scss dist/css/index.css"
  },
  "devDependencies": {
    "sass": "^1.0.0"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  }
}